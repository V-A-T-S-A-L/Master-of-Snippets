{
    "version": 2,
    "builds": [
      { "src": "server/api/**/*.js", "use": "@vercel/node" },
      { "src": "frontend/build/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/build/$1" }
    ]
  }
  